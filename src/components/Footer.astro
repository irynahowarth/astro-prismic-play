---
import { generateSetting } from "../prismic";

const { title, navigation } = await generateSetting();
---

<footer>
  {title}
  <div>
    <ul>
      {
        navigation.length &&
          navigation.map(({ link, label }) => (
            <li>
              <a href={link.url}>{label}</a>
            </li>
          ))
      }
    </ul>
  </div>
</footer>
